/*重置样式*/
*{
	padding: 0;
	margin: 0;
}
body{
	font-family: 'Microsoft Yahei';
	font-size: 12px;
	background: #f6f6f6;
}
ul{
	list-style: none;
}
a{
	text-decoration: none;
	color: #999;
}
a:hover{
	color: #c81623;
}
span{
	color: #999;
}
img{
	border:none;
	cursor: pointer;
}
.left{
	float: left;
}
.right{
	float: right;
}
/*公共图片导航*/
#commonNav{
	margin: 30px auto;
	width: 100%;
	background: e0e0e0;
}
#normal{
	margin: 30px auto;
	width: 100%;
	height: 100px;
	background: #e0e0e0;
	text-align: center;
}
#normal img{
	display: inline-block;
	cursor: pointer;
	transition: all linear .3s;
}
#normal img:hover{
	opacity: .7;
}
/*公共标题*/
#title{
	width: 1190px;
	margin: 0 auto;
	text-align: center;
	margin-bottom: 20px;
	background: url(../img/dianbg.png);
}
#title h3{
	width: 178px;
	line-height:31px; 
	display: inline;
    padding: 0 50px;
    background-color: #f6f6f6;
    font-weight: 400;
    font-size: 26px;
    color: #333;
    font-family: fzzhengheis-el-gbregular;
}
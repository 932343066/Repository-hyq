@charset "utf-8";

/*全局设定*/
*{
	padding: 0;
	margin: 0;
	border: none;
}
a{
	text-decoration: none;
}
ul{
	list-style: none;
}
/*全局设定结束*/
#box{
	width: 1210px;
	margin: 0 auto;
	position: relative;
}
/*顶部开始*/
/*背景开始*/
#top_bg{
	width: 100%;
	height: 30px;
	background: #F1F1F1;
}
.top{
	width: 1210px;
	height: 30px;
	margin: 0 auto;
	line-height: 30px;
	font-size: 12px;
	color: #666;
	font-family: Arial,Verdana,"宋体";
	position: relative;
}

.top .sit{
	width: 94px;
	height: 30px;
	position: absolute;
	top: 0px;
	left: 0px;
	border: 1px solid transparent;
	border-top: none;
}
.top .sit div{
	padding-left: 9px;
}


.top ul{
	float: right;
	line-height: 30px;
}
.top ul li{
	float: left;
	text-align: center;
	height: 30px;
	font-family: Arial,Verdana,"宋体";
	font-size: 12px;
}
.top ul li a{
	color: #666;
}
.top ul li a:hover{
	color: #C81623;
}
.top ul li.space{
	width: 1px;
	height: 12px;
	background: #DDD;
	line-height: 30px;
	margin-top: 9px;
}
.top ul li.reg_login{
	width: 130px;
	margin-right: 10px;
}
.top ul li.reg_login .login{
	font-family: "verdana";
	padding-right: 2px;
}
.top ul li.reg_login .login:hover{
	color: #666;
}
.top ul li.reg_login .reg{
	color: #C81623;
}
.top ul li.order{
	width: 68px;
}
.top ul li.jindong{
	width: 86px;
}
.top ul li.huiyuan{
	width: 68px;
}
.top ul li.caigou{
	width: 68px;
}
.top ul li.shoujijd{
	width: 102px;
	background: url(../images/jd2015img.png) 9px 5px no-repeat ;
}
.top ul li.shoujijd:hover{
	background: url(../images/jd2015img.png) 9px -20px no-repeat ;
}
.top ul li.guanzhu{
	width: 86px;
}
.top ul li.fuwu{
	width: 86px;
}
.top ul li.daohang{
	width: 86px;
}
/*背景结束*/

/*顶部结束oooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooooo*/

/*顶部广告开始*/
#top_ad{
	width: 100%;
	height: 80px;
	background: #CA0022;
}
.ad{
	width: 1210px;
	height: 80px;
	margin: 0 auto;
	position: relative;
	background: url(../images/56307f50N0c39f23c.jpg) 0 0 no-repeat;
}
a.ad_bg{
	display: block;
	width: 1210px;
	height: 80px;
	/*float: left;*/
}
.right_ad{
	width: 173px;
	height: 80px;
	position: absolute;
	top: 0px;
	right: 25px;
}

.right_ad li{
	width: 170px;
	height: 24px;
